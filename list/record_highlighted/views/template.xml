<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="assets" name="record_highlighted assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/record_highlighted/static/src/js/web.js"></script>
            </xpath>
        </template>

        <template id="assets_common" name="record_highlighted assets" inherit_id="web.assets_common">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/css" href="/record_highlighted/static/src/css/style.css"/>
        </xpath>
    </template>
    </data>
</odoo>
