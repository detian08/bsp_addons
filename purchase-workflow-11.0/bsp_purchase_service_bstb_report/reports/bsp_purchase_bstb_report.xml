<?xml version="1.0" encoding="utf-8"?>

<odoo>
<!--	<record id="stock_picking_print_view" model="ir.ui.view">-->
	<record id="po_service_print_dialog" model="ir.ui.view">
			<field name="name">purchase.order.service.wizard.bstb</field>
			<field name="model">purchase.order.service.wizard.bstb</field>
			<field name="arch" type="xml">
				<form string="Print BSTB - Service">
				<p class="oe_grey">
				Print Bukti Serah Terima Barang PO Service now?
				</p>
					<footer>
						<button string="Print" class="btn-primary" name="print_report" type="object" />
						<button string="Cancel" class="btn-primary" special="cancel"/>
					</footer>
				</form>
			</field>
	</record>

    <record id="po_service_print_output" model="ir.ui.view">
		<field name="name">Purchase Order Service - BSTB</field>
		<field name="model">purchase.order.service.output.bstb</field>
		<field name="arch" type="xml">
			<form string="Purchase Order Service - BSTB docx">
				<group>
					<field name="po_service_bstb_data" filename="po_service_bstb_file" />
					<field name="po_service_bstb_file" invisible="1" />
				</group>
				<footer>
					<button string="Cancel" class="btn-primary" special="cancel"/>
				</footer>
			</form>
		</field>
	</record>

	<!--Menu to Print Docx Report-->
	<act_window id="action_purchase_order_service_bstb"
				key2="client_action_multi"
				name="PO Service - Bukti Serah Terima Barang"
				res_model="purchase.order.service.wizard.bstb"
				src_model="purchase.order"
				view_mode="form"
				target="new"/>
</odoo>
