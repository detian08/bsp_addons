<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="view_account_invoice_tax_wizard_form" model="ir.ui.view">
            <field name="name">account.invoice.tax.wizard.form</field>
            <field name="model">account.invoice.tax.wizard</field>
            <field name="arch" type="xml">
                <form string="Unvalid taxes">
                    <sheet>
                        <field name="errors"/>
                        <field name="invoices_in_error" invisible="1"/>
                    </sheet>
                    <footer>
                        <button name="force_open_invoice" type="object"
                            string="Force invoice opening"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>
